<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.CityDAO">
  <select id="findById" resultType="City">
    select * from city where id =
    #{id}
  </select>
  
    <select id="findAll" resultType="City">
    select * from city
  </select>

  <insert id="insert" parameterType="City">
    insert into city(name, create_time, deleted)
    values(#{name}, #{createTime},0)
  </insert>
</mapper>